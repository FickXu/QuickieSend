.collect-list .list-order-item {
  background-color: #fff;
  padding: 0 20rpx 30rpx 20rpx;
  margin: 0 30rpx;
  border-radius: 30rpx;
}
.collect-list .list-order-item:not(:first-child) {
  margin-top: 20rpx;
}
.collect-list .list-order-item-body {
  width: 100%;
  display: inline-flex;
  padding: 30rpx;
  border-bottom: 1px solid #e2e2e2;
}
.collect-list .list-order-item-body .shop-view {
  min-width: 200rpx;
  max-width: 200rpx;
  min-height: 200rpx;
  max-height: 200rpx;
}
.collect-list .list-order-item-body .shop-view .img {
  width: 100%;
  height: 100%;
  padding: 5px;
  border-radius: 5px;
  border: 1px solid #f7f7f7;
}
.collect-list .list-order-item-body .shop-attr {
  width: 100%;
  margin-left: 48rpx;
}
.collect-list .list-order-item-body .shop-attr-shape {
  color: #3d3d3d;
}
.collect-list .list-order-item-body .shop-attr-specs {
  color: #b3b3b3;
  margin-top: 10rpx;
}
.collect-list .list-order-item-body .shop-attr-total {
  width: 100%;
  margin-top: 10rpx;
}
