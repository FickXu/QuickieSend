.label {
  margin: 30rpx 0rpx 40rpx;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #111111;
}
.coupon-item {
  margin: 20rpx 0 0;
  display: flex;
  justify-content: space-between;
}
.coupon-item .light-mageta {
  color: #F98490;
}
.coupon-item .magenta {
  color: #f23131;
}
.coupon-item .magenta-linear {
  box-shadow: 6px 0rpx 6px rgba(217, 58, 85, 0.2);
}
.coupon-item .magenta-linear::before {
  background-color: #f7f7f7;
}
.coupon-item .magenta-linear::after {
  background: linear-gradient(270deg, rgba(248, 132, 156, 0.2) 0%, #ef5668 50%);
}
.coupon-item .magenta-linear-full {
  background: linear-gradient(270deg, #f8849c 0%, #ef5668 100%);
}
.coupon-item .color-999 {
  color: #999999;
}
.coupon-item .bg-999-content {
  position: absolute;
}
.coupon-item .bg-999-content::before {
  background-color: #f7f7f7;
}
.coupon-item .bg-999-content::after {
  background: #999999;
}
.coupon-item .bg-999 {
  background: #999999;
}
.coupon-item .coupon-body {
  flex: 8;
  height: 192rpx;
  background: #ffffff;
  position: relative;
  z-index: 1;
  border-radius: 12rpx;
  display: flex;
  align-items: center;
}
.coupon-item .coupon-body::before {
  content: '';
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  top: 71rpx;
  left: -30rpx;
  border-radius: 100rpx;
}
.coupon-item .coupon-body::after {
  content: '';
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  top: 71rpx;
  right: -30rpx;
  border-radius: 100rpx;
}
.coupon-item .coupon-body .left {
  flex: 5;
  margin-left: 60rpx;
  position: relative;
}
.coupon-item .coupon-body .left::after {
  content: '';
  position: absolute;
  top: 16rpx;
  right: 0;
  width: 2rpx;
  height: 124rpx;
  background-color: #f5f5f5;
}
.coupon-item .coupon-body .left .money .symbol {
  font-size: 60rpx;
  font-weight: 400;
}
.coupon-item .coupon-body .left .money .num {
  font-size: 100rpx;
  font-weight: 400;
}
.coupon-item .coupon-body .left .expired {
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999999;
}
.coupon-item .coupon-body .right {
  flex: 6;
  padding: 0 30rpx;
}
.coupon-item .coupon-body .right .title {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
  margin-bottom: 10rpx;
}
.coupon-item .coupon-body .right .btn-rule-info {
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 400;
}
.coupon-item .btn-pull-coupon {
  flex-basis: 132rpx;
  height: 192rpx;
  border-radius: 16rpx;
  margin-left: -30rpx;
  position: relative;
  text-align: center;
}
.coupon-item .btn-pull-coupon .btn-pull-txt {
  position: absolute;
  top: 18rpx;
  width: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  line-height: 40rpx;
}
.list-order-none {
  text-align: center;
  color: #888888;
  background-color: #fff;
  padding: 0 20rpx 30rpx 20rpx;
  margin: 10px 30rpx;
  border-radius: 30rpx;
}
.list-order-none image {
  height: 600rpx;
}
