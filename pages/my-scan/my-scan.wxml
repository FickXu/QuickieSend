<view class="my-scan">
  <cu-custom isBack="{{true}}">
    <view slot="content" class="bold">{{ title }}</view>
  </cu-custom>

  <!-- 二维码 -->
  <view class="scan mt-20">
    <image src="../images/rich.png" alt="扫我"  />
  </view>

  <!-- 操作按钮 -->
  <view class="mt-20">
    <button class="button bg-fill" open-type='getUserInfo' bindgetuserinfo="bindgetuserinfo">购物详情</button>
  </view>
  <view class="mt-20">
    <button class="button border" bindtap="pageBack">用户消费记录</button>
  </view>
</view>